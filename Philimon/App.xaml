<Application x:Class="Philimon.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Philimon"
             StartupUri="Main.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="wpf_styles/ph_Slider.xaml"/>
                <ResourceDictionary Source="wpf_styles/ph_Button.xaml"/>
                <ResourceDictionary Source="wpf_styles/ph_checkbox.xaml"/>
                <ResourceDictionary Source="wpf_styles/ph_Images.xaml"/>
                <ResourceDictionary Source="wpf_styles/ph_progressbar.xaml"/>
                <ResourceDictionary Source="wpf_styles/ph_TextBox.xaml"/>


            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
